@model IEnumerable<NFTCollectionListingViewModel>

<div class="row">

</div>

<div class="row">
    @foreach (var nft in Model)
    {
       <div class="col-md-4 col-lg-3 col-sm-4 col-xl-3">
            <div class="card mb-3">
            <img class="card-img-top" src="@nft.ImageUrl" alt="@nft.Name">
            <div class="card-body text-center">
                <h5 class="card-title text-center">@nft.Name</h5>
                <a asp-controller="NFTCollections" asp-action="Details" asp-route-id="@nft.Id" class="btn btn-primary">Details</a>
            </div>
        </div>
       </div>

       
    }
</div>

